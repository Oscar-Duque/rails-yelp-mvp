<%= render "navbar" %>

<h1>Your review:</h1>

<%= simple_form_for [ @restaurant, @review ] do |f| %>
  <%= f.error_notification %>
  <%= f.input :rating %>
  <%= f.input :content %>
  <%= f.button :submit, "Submit Form", class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>

