<%= render "navbar" %>

<h1><%= @restaurant.name %></h1>
<ul>
    <li>Category: <%= @restaurant.category %></li>
    <li>Address: <%= @restaurant.address %></li>
    <li>Phone Number: <%= @restaurant.phone_number %></li>
</ul>
<%= link_to "Leave a review", new_restaurant_review_path(@restaurant), class: "btn btn-primary"%>
<%= link_to "Go back", restaurants_path, class: "btn btn-secondary"%>
