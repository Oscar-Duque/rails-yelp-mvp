
<%= simple_form_for restaurant, do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone-number %>
  <%= f.input :category as: :check_boxes, collection: Restaurant::CATEGORY, wrapper: :vertical_collection_inline %>
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
